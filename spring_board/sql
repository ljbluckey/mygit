CREATE TABLE TBLBOARD(
SEQ NUMBER  PRIMARY KEY,--글번호
TITLE VARCHAR2(200),--글제목
CONTENT VARCHAR2(2000),--글내용
WRITER VARCHAR2(100),--글작성자
REGDATE VARCHAR2(14),--글등록일
HITCOUNT NUMBER,--조회수
PASSWORD VARCHAR2(10),--글암호
BREF NUMBER DEFAULT 0, --참조원글번호
BSTEP NUMBER DEFAULT 0,--글순번
BLEVEL NUMBER DEFAULT 0,--글 들여쓰기 수준 ,게시파일업로드경로
UPLOADPATH VARCHAR2(100)
);
--  글번호를 1부터 시작하고 1씩증가시키는 시퀀스 
CREATE SEQUENCE SEQ_BOARD;